/*Question-14:

			Write nested loop to draw this pattern

			##
			#  #
			#   #
			#    #
			#     #
			#      #
			#        #
*/
		
		
		 for(int i =1;i<=7;i++) {
				
			   for(int j =i;j<=i;j++) {
				System.out.print("#");
				for(int y=1;y<=i;y++) {
					System.out.print(" ");
				}				
			    }
			System.out.println("#");
			
		}	
	
	
		
	}


}

